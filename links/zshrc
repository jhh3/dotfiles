# Path to your oh-my-zsh installation.
export ZSH=~/.oh-my-zsh

# oh-my-zsh theme
ZSH_THEME="jhh3"

# oh-my-zsh plugins
plugins=(git docker)

# User configuration
DEFAULT_USER=`id -un`
source $ZSH/oh-my-zsh.sh

# Editor
export EDITOR='nvim'
export VIM_PYTHON='/Users/jhh3/.virtualenvs/vim/bin/python'

# Settings
setopt nobeep

source ~/.zsh/alias.zsh
source ~/.zsh/functions.zsh
source ~/.zsh/env.zsh
# Load local variables
if [[ -a ~/.zsh/.localrc ]]
then
  source ~/.zsh/.localrc
fi

# PATH variable
export PATH=/usr/texbin:/Library/TeX/texbin:$PATH
